<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.dayou.com.baoma.view.myview.customview.TitleBar
        android:id="@+id/benefittitle"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <include layout="@layout/linesplite"></include>

    <ListView
        android:paddingTop="@dimen/dp32"
        android:clipToPadding="false"
        android:id="@+id/benefitlistview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@color/white"
        android:dividerHeight="@dimen/dp6"
        android:paddingBottom="@dimen/dp40"
        android:scrollbars="none" />
</LinearLayout>