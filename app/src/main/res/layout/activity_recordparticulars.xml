<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"

    android:layout_height="match_parent">
    <include layout="@layout/layout_head_2" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:background="@color/white"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/jiancha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="妇科档案"
            android:padding="@dimen/layout_margin"
            style="@style/ZhuTextwhite"/>

            />
        <TextView
            android:id="@+id/createTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/layout_margin"
            android:text="1991/11/13"
            android:gravity="right"
            style="@style/ZhuTextwhite"

            android:drawablePadding="@dimen/padding5"/>
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:overScrollMode="never"

        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"


            >
            <include

                layout="@layout/include_recordparticulars1"
                />
            <include

                layout="@layout/include_recordparticulars11"

                />
            <include

                layout="@layout/include_recordparticulars2"

                />
            <include

                layout="@layout/include_recordparticulars3"
                />




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/layout_margin"
                android:paddingRight="@dimen/layout_margin"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15black"
                    android:layout_marginRight="-10dp"
                    android:text="检查项目"
                    android:gravity="left|center"
                    android:paddingLeft="@dimen/padding5">

                </TextView>
                <ListView
                    android:id="@+id/list"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/llstatu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/layout_margin"

                android:paddingRight="@dimen/layout_margin"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15black"
                    android:layout_marginRight="-10dp"
                    android:text="状态"
                    android:gravity="left"
                    android:paddingLeft="@dimen/padding5">

                </TextView>

                <TextView
                    android:id="@+id/statu"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15grey"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5">
                </TextView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llzhenduan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/layout_margin"
                android:paddingRight="@dimen/layout_margin"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15black"
                    android:layout_marginRight="-10dp"
                    android:text="诊断"
                    android:gravity="left"
                    android:paddingLeft="@dimen/padding5">

                </TextView>

                <TextView
                    android:id="@+id/zhenduan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15grey"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5">
                </TextView>
            </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/layout_margin"
        android:paddingRight="@dimen/layout_margin"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15black"
            android:layout_marginRight="-10dp"
            android:text="健康指导"
            android:gravity="left"
            android:paddingLeft="@dimen/padding5">

        </TextView>

        <TextView
            android:id="@+id/care"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15grey"
           android:gravity="center|left"
            android:paddingLeft="@dimen/padding5">
        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/layout_margin"
        android:paddingRight="@dimen/layout_margin"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15black"
            android:layout_marginRight="-10dp"
            android:text="主诊医生"
            android:gravity="center|left"
            android:paddingLeft="@dimen/padding5">

        </TextView>

        <TextView
            android:id="@+id/doctor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15grey"
            android:gravity="center|left"
            android:paddingLeft="@dimen/padding5">
        </TextView>
    </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/layout_margin"
                android:paddingRight="@dimen/layout_margin"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15black"
                    android:layout_marginRight="-10dp"
                    android:text="接待客服"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5">

                </TextView>

                <TextView
                    android:id="@+id/kefu1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="3"
                    android:background="@drawable/table_frame_gray"
                    style="@style/TextStyle15grey"
                    android:gravity="center|left"
                    android:paddingLeft="@dimen/padding5">
                </TextView>
            </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/layout_margin"
        android:paddingRight="@dimen/layout_margin"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15black"
            android:layout_marginRight="-10dp"
            android:text="就诊医院"
            android:gravity="center|left"
            android:paddingLeft="@dimen/padding5">

        </TextView>

        <TextView
            android:id="@+id/hospital"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:background="@drawable/table_frame_gray"
            style="@style/TextStyle15grey"
            android:gravity="center|left"
            android:paddingLeft="@dimen/padding5">
        </TextView>
    </LinearLayout>
    <TextView
        android:id="@+id/nexttime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/layout_margin"
        android:layout_marginRight="@dimen/layout_margin"
        android:layout_marginBottom="@dimen/layout_margin"
        android:text="aaaa"
        style="@style/TextStyle15black"
        android:background="@drawable/table_frame_gray"
        android:gravity="center|left"
        android:paddingLeft="@dimen/padding5"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>