<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/layout_margin60"
    android:layout_marginRight="@dimen/layout_margin60"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen48"
        android:background="@color/pinkbg"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/layout_margin"
        android:text="@string/import_obstetrics"
        android:textColor="@color/background_white" />

    <android.dayou.com.baoma.view.myview.WheelView
        android:id="@+id/wheelview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></android.dayou.com.baoma.view.myview.WheelView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/layout_margin2"
        >

        <TextView
            android:id="@+id/cancel_d"
            android:layout_width="@dimen/butwidth"
            android:layout_height="@dimen/height2"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/margin6"
            android:gravity="center"
            android:text="取消"
            android:textColor="@color/pinkbg"
            android:textSize="@dimen/size_15" />

        <TextView
            android:id="@+id/sure_d"
            android:layout_width="@dimen/butwidth"
            android:layout_height="@dimen/height2"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/margin6"
            android:gravity="center"
            android:text="确定"
            android:textColor="@color/pinkbg"
            android:textSize="@dimen/size_15" />
    </RelativeLayout>
</RelativeLayout>