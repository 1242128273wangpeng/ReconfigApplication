<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_show_web_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#df000000"
    tools:context="android.dayou.com.baoma.view.activity.ShowWebImageActivity">

    <android.dayou.com.baoma.album.ui.widget.HackyViewPager
        android:id="@+id/mood_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/bigimg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:scaleType="center" />
    </android.dayou.com.baoma.album.ui.widget.HackyViewPager>

</RelativeLayout>
